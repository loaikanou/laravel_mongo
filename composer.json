{
    "name": "loaikanou/laravel_mongo",
    "description": "MongoDB driver, auth, passport, for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Loai N Kanou",
            "email": "Loai.net@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "mongodb/mongodb": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "loaikanou\\laravel_mongo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "loaikanou\\laravel_mongo\\MongoServiceProvider"
            ],
            "aliases": {
                "Mongo": "loaikanou\\laravel_mongo\\MongoFacade"
            }
        }
    }
}
